{
  "expo": {
    "name": "react_native_typescript_form",
    "slug": "react_native_typescript_form",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
			"bundleIdentifier": "com.daekyocns.rn-form",
			"buildNumber": "1.0.0",
			"requireFullScreen": true
    },
    "android": {
      "package": "com.daekyocns.rn-form",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
